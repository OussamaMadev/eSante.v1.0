<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="eSantePU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>esante.Reservations</class>
    <class>esante.DemandeAnnulationMed</class>
    <class>esante.Patients</class>
    <class>esante.Employes</class>
    <class>esante.Consultations</class>
    <class>esante.Patients_1</class>
    <class>esante.DemandeAnnulationMed_1</class>
    <class>esante.Employes_1</class>
    <class>esante.Consultations_1</class>
    <class>esante.Clininfos_1</class>
    <class>esante.Clininfos</class>
    <class>entities.Consultations</class>
    <class>entities.Reservations</class>
    <class>entities.Patients</class>
    <class>entities.Employes</class>
    <class>entities.DemandeAnnulationMed</class>
    <class>entities.Clininfos</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/mysql?zeroDateTimeBehavior=CONVERT_TO_NULL"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.cj.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value=""/>
      <property name="javax.persistence.schema-generation.database.action" value="create"/>
    </properties>
  </persistence-unit>
</persistence>
